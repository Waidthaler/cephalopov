        minimum_reuse 2.1
        nearest_count 15
        normal true
        pretrace_start 0.8
        pretrace_end 0.2
        recursion_limit 5
        subsurface true
    }
    subsurface {
        radiosity true
        samples 7,19
    }
    photon {
        count 66
        gather 5,8
        media 5.6,0.222
        jitter 1.2
